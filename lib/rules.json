[
    {
      "port": "generator",
      "worker": "ServerlessWorker"
    },
    {
      "port": "new-project",
      "worker": "NewServerlessProjectWorker"
    },
  
    {
      "port": "custom-name-table",
      "template": "yml/customTable.yml"
    },
  
    {
      "port": "permissions-table",
      "template": "yml/permissions.yml"
    },
  
    {
      "port": "environment-template",
      "template": "yml/environments.yml"
    },
  
    {
      "port":"lambda-functions-template",
      "template": "yml/functions.yml"
    },
  
    {
      "port":"resources-table-template",
      "template": "yml/resources.yml"
    },
    
    {
      "port":"configuration",
      "chain":["serverless-init-templates"]
    },
  
    {
      "port": "make-crud-serverless",
      "chain": ["make-entity", "make-dao", "make-lambda-functions"]
    },
  
    {
      "port":"not-found-response-service",
      "template":"response/NotFound.java"
    },
  
    {
      "port":"error-response-service",
      "template":"response/Error.java"
    },
  
    {
      "port":"success-response-service",
      "template":"response/Success.java"
    },
  
    {
      "port":"invk-to-get-properties-name",
      "template":"property/ThizProperty.java"
    },
  
    {
      "port":"invk-to-convert",
      "template":"property/ArrayPropertyToString.java"
    },
  
    {
      "port":"property-to-convert-type",
      "propertyType":"float",
      "text":"=$%f"
    },
  
    {
      "port":"property-to-convert-type",
      "propertyType":"int",
      "text":"=%d"
    },
  
    {
      "port":"property-to-convert-type",
      "propertyType":"string",
      "text":"=%s"
    },
  
  
    {
      "port":"make-lambda-functions",
      "chain":["make-lambda-create","make-lambda-list",
        "make-lambda-get","make-lambda-update","make-lambda-delete"
      ]
    },
  
    {
      "port": "make-entity",
      "template": "entity/Entity.java",
      "output": "src/main/java/${groupPath}/${projectName}/model/${entity.name}.java"
    },
  
    {
      "port":"make-dao",
      "template":"dao/DAO.java",
      "output":"src/main/java/${groupPath}/${projectName}/dao/${entity.name}DAO.java"
    },
  
    {
      "port":"make-lambda-create",
      "template":"crud/CreateHandler.java",
      "output":"src/main/java/${groupPath}/${projectName}/crud/Create${entity.name}Handler.java"
    },
  
    {
      "port":"make-lambda-list",
      "template":"crud/ListHandler.java",
      "output":"src/main/java/${groupPath}/${projectName}/crud/List${entity.name}Handler.java"
    },
  
    {
      "port":"make-lambda-get",
      "template":"crud/GetHandler.java",
      "output":"src/main/java/${groupPath}/${projectName}/crud/Get${entity.name}Handler.java"
    },
  
    {
      "port":"make-lambda-update",
      "template":"crud/UpdateHandler.java",
      "output":"src/main/java/${groupPath}/${projectName}/crud/Update${entity.name}Handler.java"
    },
  
    {
      "port":"make-lambda-delete",
      "template":"crud/DeleteHandler.java",
      "output":"src/main/java/${groupPath}/${projectName}/crud/Delete${entity.name}Handler.java"
    },
  
    {
      "port": "make-package",
      "template": "Package.java"
    },
  
    {
      "port":"set-properties-entities",
      "template":"SetPropertiesEntities.java"
  
    },
  
    {
      "port": "property-entity",
      "template": "property/Property.java"
    },
  
    {
      "port":"property-post-type",
      "propertyType":"string",
      "text":"Text"
    },
  
    {
      "port":"property-post-type",
      "propertyType":"int",
      "text":"Int"
    },
  
    {
      "port":"property-post-type",
      "propertyType":"float",
      "text":"Double"
    },
  
    {
      "port": "property-type",
      "propertyType": "int",
      "text": "Integer"
    },
  
    {
      "port": "property-type",
      "propertyType": "string",
      "text": "String"
    },
  
    {
      "port": "property-type",
      "propertyType": "text",
      "text": "Text"
    },
  
    {
      "port": "property-type",
      "propertyType": "float",
      "text": "Float"
    },
  
    {
      "port": "property-type",
      "propertyType": "boolean",
      "text": "Boolean"
    },
  
    {
      "port": "property-get-range-key",
      "template": "property/GetPropertyRangeKey.java"
    },
  
    {
      "port": "property-set-range-key",
      "template": "property/SetPropertyRangeKey.java"
    },
  
    {
      "port": "property-get",
      "template": "property/GetProperty.java"
    },
  
    {
      "port": "property-set",
      "template": "property/SetProperty.java"
    },
  
    {
      "port":"serverless-init-templates",
      "template-batch": [
          {
            "template": "pom.xml",
            "output": "pom.xml"
          },
  
          { 
            "template": "ApiGatewayResponse.java",
            "output": "src/main/java/${groupPath}/${projectName}/ApiGatewayResponse.java"
          },
  
          { 
            "template": "Response.java",
            "output": "src/main/java/${groupPath}/${projectName}/Response.java"
          },
  
          { 
            "template": "ApiGatewayResponse.java",
            "output": "src/main/java/${groupPath}/${projectName}/ApiGatewayResponse.java"
          },
  
          {
              "template": "database/log4j2.xml",
              "output": "src/main/resources/log4j2.xml"
          },
          {
              "template": "serverless.yml",
              "output": "serverless.yml"
          }
        ]
  
    }
  ]